{
    "manifest_version": 3,
    "name": "YouTube Playlist Tick Marker",
    "version": "1.0",
    "description": "Mark videos as completed in YouTube playlists and save your progress.",
    "permissions": ["storage"],
    "host_permissions": ["https://www.youtube.com/*"],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/playlist?*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }
  